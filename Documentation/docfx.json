{
    "metadata": [
        {
            "src": [
                {
                    //"src": "../Amoebotsim 2.0",
                    "src": "../Amoebotsim 2.0/Assets",
                    "files": [
                        //"**.csproj"
                        "**.cs"
                    ]
                }
            ],
            "dest": "api",
            "disableGitFeatures": false,
            //"disableDefaultFilter": true,
            "filter": "filterConfig.yml",
            "globalNamespaceId": "Global"
        }
    ],
    "build": {
        "content": [
            {
                "files": [
                    "api/**.yml",
                    "api/index.md"
                ]
            },
            {
                "files": [
                    //"articles/**.md",
                    //"articles/**/toc.yml",
                    //"installation_guide/**.md",
                    //"installation_guide/**/toc.yml",
                    //"nested_pages/**.md",
                    //"nested_pages/**/toc.yml",
                    "toc.yml",
                    "*.md"
                ]
            },
            {
                "src": "amoebot_model",
                "files": [ "**.md", "**.yml" ],
                "dest": "amoebot_model"
            },
            {
                "src": "dev_guide",
                "files": [ "**.md", "**.yml" ],
                "dest": "dev_guide"
            },
            {
                "src": "installation_guide",
                "files": [ "**.md", "**.yml" ],
                "dest": "installation_guide"
            },
            {
                "src": "model_ref",
                "files": [ "**.md", "**.yml" ],
                "dest": "model_ref"
            },
            {
                "src": "nested_pages",
                "files": [ "**.md", "**.yml" ],
                "dest": "nested_pages"
            },
            {
                "src": "user_guide",
                "files": [ "**.md", "**.yml" ],
                "dest": "user_guide"
            }
        ],
        "resource": [
            {
                "files": [
                    "images/**"
                ]
            }
        ],
        "overwrite": [
            {
                "files": [
                    "apidoc/**.md"
                ],
                "exclude": [
                    "obj/**",
                    "_site/**"
                ]
            }
        ],
        "dest": "_site",
        "globalMetadata": {
            "_appTitle": "Amoebotsim 2.0",
            "_appFooter": "(TODO: Footer) Example Unity documentation",
            "_enableSearch": true
        },
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [
            "statictoc",
            // This custom template removes the list of inherited members from class documentation pages
            "templates/custom"
        ],
        //"template": ["default", "templates/unity"],
        "postProcessors": [ "ExtractSearchIndex" ],
        "markdownEngineName": "markdig",
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false
    }
}